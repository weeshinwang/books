{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "array-type": false,
    "interface-name": false,
    "interface-over-type-literal": false,
    "max-classes-per-file": false,
    "member-access": false,
    "no-console": false,
    "no-namespace": false,
    "no-shadowed-variable": false,
    "no-unused-expression": false,
    "prefer-const": false,
    "quotemark": [true, "single"],
    "semicolon": false
  },
  "rulesDirectory": []
}